# ========================================================================
# ENVIRONMENT CONFIGURATION EXAMPLE
# ========================================================================
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# ========================================================================
# NODE ENVIRONMENT
# ========================================================================
NODE_ENV=development

# ========================================================================
# SUPABASE CONFIGURATION
# ========================================================================
# Get these values from: https://app.supabase.com/project/<your-project>/settings/api

# Project URL (required)
SUPABASE_URL=https://your-project-id.supabase.co

# Public anon key (required for frontend/client-side)
# This key is safe to use in client-side code
SUPABASE_ANON_KEY=your_anon_key_here

# Service role key (optional, only for backend/server-side)
# ⚠️ NEVER expose this key in client-side code!
SUPABASE_SERVICE_KEY=your_service_role_key_here

# Legacy keys (for backward compatibility)
SUPABASE_ANON_API_KEY=your_anon_key_here
SUPABASE_SERVICE_API_KEY=your_service_role_key_here
SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here

# ========================================================================
# EXTERNAL APIS (Optional)
# ========================================================================
COSMOS_API_TOKEN=your_cosmos_token_here
RECEITA_WS_URL=https://www.receitaws.com.br/v1/cnpj
BRASIL_API_URL=https://brasilapi.com.br
