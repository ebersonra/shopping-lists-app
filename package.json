{
  "name": "shopping-lists-app",
  "version": "1.0.0",
  "description": "Uma aplicação moderna de listas de compras com design responsivo e tema escuro",
  "main": "index.html",
  "author": "Shopping Lists Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "dotenv": "^17.2.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "netlify-cli": "^22.2.2",
    "supertest": "^7.1.4",
    "live-server": "^1.2.2",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "start": "live-server . --port=3000 --open=/index.html",
    "dev": "netlify dev",
    "build": "echo 'Build completed - static files ready for deployment'",
    "test": "node --test tests/",
    "test:unit": "node --test tests/ --reporter=spec",
    "test:integration": "node --test tests/ --grep='integration'",
    "test:watch": "node --test --watch tests/",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "netlify deploy --prod",
    "preview": "netlify deploy"
  },
  "keywords": [
    "shopping-lists",
    "web-app",
    "javascript",
    "supabase",
    "netlify",
    "dark-theme",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/shopping-lists-app.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}